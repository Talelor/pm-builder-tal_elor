              "value": "={{ parseFloat($('Confidence Assessment').item.json.output) }}",
              "type": "number"
            },
            {
              "id": "reasoning",
              "name": "reasoning",
              "value": "=Ticket classified as {{ $('Team Classifier').item.json.category }} based on: {{ $('Ticket Analysis').item.json.output.substring(0, 200) }}",
              "type": "string"
            },
            {
              "id": "priority",
              "name": "priority",
              "value": "={{ $runIndex === 0 ? 'critical' : $runIndex === 1 ? 'high' : 'normal' }}",
              "type": "string"
            },
            {
              "id": "vip_override",
              "name": "vip_override",
              "value": "={{ $('Extract Ticket Data').item.json.vip_flag }}",
              "type": "boolean"
            },
            {
              "id": "secondary_topics",
              "name": "secondary_topics",
              "value": "={{ $('Multi-Topic Handling').item.json.output === 'none' ? [] : $('Multi-Topic Handling').item.json.output.split(',').map(t => t.trim()) }}",
              "type": "array"
            }
          ]
        },
        "options": {}
      },
