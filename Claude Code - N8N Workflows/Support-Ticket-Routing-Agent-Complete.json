              "type": "string"
            },
            {
              "id": "customer_id",
              "name": "customer_id",
              "value": "={{ $json.customer_id }}",
              "type": "string"
            },
            {
              "id": "account_tier",
              "name": "account_tier",
              "value": "={{ $json.account_tier }}",
              "type": "string"
            },
            {
              "id": "vip_flag",
              "name": "vip_flag",
              "value": "={{ $json.vip_flag }}",
              "type": "boolean"
            },
            {
              "id": "created_at",
              "name": "created_at",
              "value": "={{ $json.created_at }}",
